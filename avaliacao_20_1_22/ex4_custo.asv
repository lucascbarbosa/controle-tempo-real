clear all;

% configuração inicial

G = tf([1],[1 0 0]);
H = c2d(G,2,'tustin');
[num,den] = tfdata(H); 
num = num{1};
den = den{1};
[Phi,Gamma,C,D] = tf2ss(num,den);

x=[[0;0]];
N = 100;
u = ones(1,N);

Phi = [0 1;-1 2];



for k=1:N
    x(:,k+1) = Phi*x(:,k) + Gamma*u(k);
    y(k) = C*x(:,k)+D*u(k);
end